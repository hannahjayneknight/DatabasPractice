{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "firefox",
            "request": "launch",
            "reAttach": false,
            "name": "Launch ListOfQuizzes.html",
            "file": "${workspaceFolder}/listing/ListOfQuizzes.html"
        },
        {
            "name": "Firefox Debugger",
            "type": "firefox",
            "request": "launch",
            "reAttach": false,
            "url": "http://localhost:1711/ListOfQuizzes.html",
            "webRoot": "${workspaceFolder}/listing",
            "keepProfileChanges": false
        },
        {
            "name": "Node Server",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/listing/server.js"
        },
        {
            "name": "DB Test",
            "type": "node",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/testing/databasepractice5.js"
        }
    ]
}